package jp.monyone.verify.yukicoder.No519.MaximumMatchingDP;

public class Main {

 	//@paste Graph.MaximumMatchingDP

	public static void main(String[] args) {
		try (Scanner sc = new Scanner(System.in)) {
			final int n = sc.nextInt();
			
			final int max = 1000;
			long[][] adj = new long[n][n];
			for(int i = 0; i < n; i++){
				for(int j = 0; j < n; j++){
					adj[i][j] = max - sc.nextInt();
				}
			}
		
			final int solve = MaximumGeneralMatchingDP(adj);
			System.out.println(max * (n / 2) - solve);
		}
	}
	
	//@paste Template.Scanner
	
}
